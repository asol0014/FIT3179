{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Proportional symbol map of flights at Australian airports.",
  "data": {
    "url": "../data/aircraft.csv"
  },
  "projection": {"type": "mercator"},
  "layer": [
    {
        "data": {
        "url": "../js/aus_states.json",
        "format": {"type": "topojson", "feature": "states"}
      },
      "mark": {"type": "geoshape", "fill": "transparent", "stroke": "#7063e2", "strokeWidth": 1}
    } 
  ]     
}

